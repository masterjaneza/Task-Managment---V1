{
  "builds": [
    {
      "src": "src/style.css",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "redirects": [
    { "source": "/", "destination": "/pages/registration.html", "permanent": false },
    { "source": "/index.html", "destination": "/pages/login.html", "permanent": false },
    { "source": "/products.html", "destination": "/pages/login.html", "permanent": false }
  ]
}
